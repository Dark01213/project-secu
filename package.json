{
  "name": "secure-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:https": "concurrently \"next dev -p 3000\" \"npx local-ssl-proxy --source 3443 --target 3000 --cert ./localhost.pem --key ./localhost-key.pem\"",
    "smoke": "node scripts/smoke-flow.js",
    "audit": "npm audit --json > audits/npm-audit.json",
    "seed": "node scripts/seed-admin.js",
    "build": "next build",
    "start": "next start -p 3000"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie": "^1.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "next": "^16.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "^9.0.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "local-ssl-proxy": "^2.0.5",
    "mongodb-memory-server": "^10.4.0",
    "selfsigned": "^5.2.0"
  }
}
